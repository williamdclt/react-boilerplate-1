{
  "name": "react-boilerplate",
  "version": "1.1.0",
  "description": "A highly scalable, offline-first foundation with the best DX and a focus on performance and best practices",
  "repository": {
    "type": "git",
    "url": "git://github.com/Theodo-UK/react-boilerplate.git"
  },
  "private": false,
  "engines": {
    "npm": ">=3",
    "node": ">=5"
  },
  "author": "William Duclot",
  "scripts": {
    "prebuild": "rimraf ./build",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.config.js --color --progress --hide-modules --display-optimization-bailout",
    "start": "webpack-dashboard -- webpack-dev-server --config internals/webpack/webpack.dev.config.js",
    "generate": "plop --plopfile internals/generators/index.js",
    "test:clean": "rimraf ./coverage",
    "jest": "jest --config internals/testing/jest.config.js",
    "test:lint": "eslint --ext js app/",
    "test:jest": "cross-env NODE_ENV=test jest --config internals/testing/jest.config.js",
    "test:e2e": "cross-env NODE_ENV=test jest --config internals/testing/jest.e2e.config.js",
    "test:watch": "yarn test:jest --watchAll",
    "test": "yarn test:lint && yarn test:jest --coverage"
  },
  "pre-commit": "test:lint",
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "5.2.0",
    "history": "4.7.2",
    "intl": "1.2.5",
    "prop-types": "15.6.1",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-intl": "^2.4.0",
    "react-redux": "5.0.7",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "4.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-saga": "0.16.0",
    "reselect": "3.0.1",
    "styled-components": "^3.3.2",
    "whatwg-fetch": "2.0.4"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "2.1.3",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.1",
    "babel-loader": "7.1.5",
    "babel-plugin-dynamic-import-node": "1.2.0",
    "babel-plugin-react-intl": "2.4.0",
    "babel-plugin-react-transform": "3.0.0",
    "babel-plugin-styled-components": "1.5.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.22",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "circular-dependency-plugin": "5.0.2",
    "css-loader": "0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-webpack": "0.10.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.9.1",
    "eslint-plugin-redux-saga": "^0.9.0",
    "eventsource-polyfill": "0.9.6",
    "exports-loader": "0.7.0",
    "file-loader": "1.1.11",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "image-webpack-loader": "4.3.1",
    "imports-loader": "0.8.0",
    "inquirer-directory": "2.1.0",
    "jest-cli": "^23.1.0",
    "node-plop": "0.15.0",
    "offline-plugin": "5.0.5",
    "plop": "2.0.0",
    "pre-commit": "1.2.2",
    "prettier": "^1.13.5",
    "puppeteer": "1.5.0",
    "react-test-renderer": "^16.4.1",
    "redux-mock-store": "^1.5.3",
    "rimraf": "2.6.2",
    "style-loader": "0.21.0",
    "url-loader": "1.0.1",
    "webpack": "4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-dashboard": "2.0.0",
    "webpack-dev-server": "3.1.4"
  }
}
